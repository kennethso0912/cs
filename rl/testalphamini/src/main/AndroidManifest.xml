    package="com.ubtechinc.testalphamini">

    <uses-permission android:name="com.ubtrobot.master.permission.MASTER" />

    <application
        android:name=".TestApp"
        android:allowBackup="true" android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:supportsRtl="true">
        <activity android:name=".LedTestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data android:name="mst-conn-mod" android:value="MST_APPLICATION" />
    </application>

</manifest>